<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Inicio.
    -->
    <changeSet id="20190705195827-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="id_situacao_id"
                                 baseTableName="inicio"
                                 constraintName="fk_inicio_id_situacao_id"
                                 referencedColumnNames="id"
                                 referencedTableName="situacao"/>

        <addForeignKeyConstraint baseColumnNames="id_sobre_dna_id"
                                 baseTableName="inicio"
                                 constraintName="fk_inicio_id_sobre_dna_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sobre_dna"/>

        <addForeignKeyConstraint baseColumnNames="id_noticias_id"
                                 baseTableName="inicio"
                                 constraintName="fk_inicio_id_noticias_id"
                                 referencedColumnNames="id"
                                 referencedTableName="noticias"/>

        <addForeignKeyConstraint baseColumnNames="id_projectos_id"
                                 baseTableName="inicio"
                                 constraintName="fk_inicio_id_projectos_id"
                                 referencedColumnNames="id"
                                 referencedTableName="projectos"/>

        <addForeignKeyConstraint baseColumnNames="id_publicacao_id"
                                 baseTableName="inicio"
                                 constraintName="fk_inicio_id_publicacao_id"
                                 referencedColumnNames="id"
                                 referencedTableName="publicacao"/>

        <addForeignKeyConstraint baseColumnNames="id_contactos_id"
                                 baseTableName="inicio"
                                 constraintName="fk_inicio_id_contactos_id"
                                 referencedColumnNames="id"
                                 referencedTableName="contactos"/>

    </changeSet>
</databaseChangeLog>
